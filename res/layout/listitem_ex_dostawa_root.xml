<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >
	 <TextView
	    android:visibility="gone"
	    android:textSize="25sp"
     	style="?android:attr/listSeparatorTextViewStyle"
        android:id="@+id/textView_separator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="16-02-2013" />
	<LinearLayout
	    android:layout_below="@+id/textView_separator"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="horizontal" >
	
	    <TextView	        
	        android:textSize="20sp"
	        android:layout_marginTop="10dp"
	        android:layout_marginBottom="10dp"
	        android:id="@+id/textView_ex_dostawa_nazwa"
	        android:layout_width="0dp"
	        android:layout_weight="1.618"
	        android:layout_height="wrap_content"
	        android:text="dostawca" />

	   
	
	    <TextView
	        android:textSize="20sp"
	        android:layout_marginTop="10dp"
	        android:layout_marginBottom="10dp"
	        android:id="@+id/textView_ex_dostawa_cena"
	        android:layout_width="0dp"
	        android:layout_weight="1"
	        android:layout_height="wrap_content"
	        android:text="dostawca" />
	
	</LinearLayout>
</RelativeLayout>
	
